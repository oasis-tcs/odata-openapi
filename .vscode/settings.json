{
  "json.schemas": [
    {
      "fileMatch": ["examples/*.openapi3.json", "tools/tests/*.openapi3.json"],
      "url": "./node_modules/@apidevtools/openapi-schemas/schemas/v3.0/schema.json"
    },
    {
      "fileMatch": ["tools/tests/*.swagger.json"],
      "url": "./node_modules/@apidevtools/openapi-schemas/schemas/v2.0/schema.json"
    }
  ],
  "xml.catalogs": ["./node_modules/odata-csdl/catalog.xml"],
  "xml.format.legacy": true,
  "mochaExplorer.esmLoader": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "cSpell.words": [
    "csdl",
    "deletable",
    "deltatoken",
    "innererror",
    "insertable",
    "lawngreen",
    "lightslategray",
    "odata",
    "openapi",
    "qname",
    "whitesmoke"
  ]
}
